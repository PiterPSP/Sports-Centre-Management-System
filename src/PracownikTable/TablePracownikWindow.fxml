<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PracownikTable.TablePracownikWindowController">
   <children>
      <TableView fx:id="tableView" layoutX="31.0" layoutY="67.0" onMouseClicked="#selectRowPracownik" prefHeight="198.0" prefWidth="538.0" />
      <Button fx:id="AddData" layoutX="86.0" layoutY="315.0" mnemonicParsing="false" onAction="#openNewPracownikWindow" prefWidth="200.0" text="Dodaj nowego pracownika" />
      <ComboBox fx:id="selectTableView" layoutX="31.0" layoutY="28.0" onAction="#changeTableView" prefWidth="150.0" />
      <TextField fx:id="searchTextBox" layoutX="202.0" layoutY="28.0" onKeyReleased="#searchPracownik" prefHeight="25.0" prefWidth="365.0" promptText="Wyszukaj" />
      <Button fx:id="countTax" disable="true" layoutX="323.0" layoutY="315.0" mnemonicParsing="false" onAction="#countAnnualTax" prefHeight="25.0" prefWidth="200.0" text="Oblicz roczny podatek" />
      <Label fx:id="taxValue" layoutX="323.0" layoutY="357.0" prefHeight="15.0" prefWidth="258.0" />
   </children>
</AnchorPane>
