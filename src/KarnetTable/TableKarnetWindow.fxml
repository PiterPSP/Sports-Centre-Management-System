<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="KarnetTable.TableKarnetWindowController">
   <children>
      <TableView fx:id="tableView" layoutX="31.0" layoutY="67.0" onMouseClicked="#selectRowKarnet" prefHeight="198.0" prefWidth="538.0" />
      <Button fx:id="AddData" layoutX="337.0" layoutY="320.0" mnemonicParsing="false" onAction="#openNewKarnetWindow" prefHeight="25.0" prefWidth="200.0" text="Dodaj karnet" />
      <ComboBox fx:id="selectTableView" layoutX="31.0" layoutY="28.0" onAction="#changeTableView" prefWidth="150.0" />
      <TextField fx:id="searchTextBox" layoutX="202.0" layoutY="28.0" onKeyReleased="#searchKarnet" prefHeight="25.0" prefWidth="365.0" promptText="Wyszukaj" />
      <Button fx:id="deleteOld" layoutX="69.0" layoutY="339.0" mnemonicParsing="false" onAction="#removeOldPasses" prefHeight="25.0" prefWidth="200.0" text="Usuń nieaktualne karnety" />
      <Button fx:id="delete" disable="true" layoutX="69.0" layoutY="305.0" mnemonicParsing="false" onAction="#deleteKarnet" prefHeight="25.0" prefWidth="200.0" text="Usuń karnet" />
   </children>
</AnchorPane>
